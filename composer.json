{
  "name": "olafnorge/secrets-provider",
  "description": "Simple helper to load a value from a mounted secret or, if not found, from an ENV var.",
  "keywords": [
    "container",
    "env",
    "secret",
    "configuration"
  ],
  "type": "library",
  "homepage": "https://github.com/olafnorge/secrets-provider",
  "license": "MIT",
  "authors": [
    {
      "name": "Volker Machon",
      "email": "olafnorge@users.noreply.github.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "illuminate/support": ">=5.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
